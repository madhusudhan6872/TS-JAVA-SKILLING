class Temp{
    public static void main(String[] args) {
        int []arr=new int[]{1,22,3,4,5,7,4,3};
        int ui=0;
        for(int i=0;i<10;i++){
            int flag=0;
            for(int j=0;j<ui;j++){
                if(arr[j]==arr[i]){
                    flag=1;
                    break;
                }
            }
            if(flag==0){
                arr[ui]=arr[i];
                ui+=1;
            }
        for(int i=0;i<ui;i++){
            System.out.print(arr[i]);
        }
        }
    }
}
